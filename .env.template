# MCP Container Test Environment - Environment Variables
# Copy this file to .env and update with your actual values

# GitHub MCP Server Configuration  
GITHUB_TOKEN=your_github_personal_access_token_here

# PostgreSQL Database Configuration
POSTGRES_DB=testdb
POSTGRES_PASSWORD=secure_postgres_password_here

# MySQL Database Configuration
MYSQL_ROOT_PASSWORD=secure_mysql_root_password_here
MYSQL_DATABASE=testdb

# MongoDB Database Configuration
MONGODB_DATABASE=testdb

# Grafana Configuration
GRAFANA_ADMIN_PASSWORD=secure_grafana_admin_password_here
GRAFANA_TOKEN=your_grafana_service_account_token_here
